<script type="text/javascript">
    function printClick() {
        var customerCode = null,
            customerName = null,
            deliverLineCode = null;


        if ($('#customerCode').val() == null) { customerCode = ""; } else { customerCode = $('#customerCode').val(); }
        if ($('#customerName').val() == null) { customerName = ""; } else { customerName = $('#customerName').val(); }
        if ($('#deliverLineCode').val() == null) { deliverLineCode = ""; } else { deliverLineCode = $('#deliverLineCode').val(); }
        window.location.href = "/Customer/CreateExcelToClient?CustomerCode=" + customerCode
                                                                              + "&CustomerName=" + customerName
                                                                              + "&DeliverLineCode=" + deliverLineCode

    }
</script>
