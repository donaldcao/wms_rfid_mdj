<script type="text/javascript">
    function printClick() {
        var distCode = null,
            customCode = null,
            distName = null,
            isActive = null;

        if ($('#searchDistCode').val() == null) { distCode = ""; } else { distCode = $('#searchDistCode').val(); }
        if ($('#searchCustomCode').val() == null) { customCode = ""; } else { customCode = $('#searchCustomCode').val(); }
        if ($('#searchDistName').val() == null) { distName = ""; } else { distName = $('#searchDistName').val(); }
        if (document.getElementById('status') == null) { isActive = ""; } else { isActive = $('#status').combo('getValue'); }
        window.location.href = "/DeliverDist/CreateExcelToClient?distCode=" + distCode
                                + "&CustomCode=" + customCode
                                + "&DistName=" + distName
                                + "&IsActive=" + isActive;
    }
</script>
