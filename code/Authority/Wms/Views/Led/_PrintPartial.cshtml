<script type="text/javascript">
    function printClick() {

        var row = $('#details').datagrid('getSelected');
        if (row) {
            var rowLedCode = null;
            if (row.LedCode == null) { rowLedCode = ""; } else { rowLedCode = row.LedCode; }
            window.location.href = '/Led/CreateExcelToClient/?LedCode=' + rowLedCode;
        } else {
            $.messager.alert('系统提示', '未选中行', "info");
        }
    }
</script>
