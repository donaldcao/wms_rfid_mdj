<script type="text/javascript">
    function printClick() {
        var DeviceCode = null,
        StateCode = null,
        BeginTime = null,
        EndTime = null;

        if ($('#DeviceCode').val() == null) { DeviceCode = ""; } else { DeviceCode = $('#DeviceCode').val(); }
        if ($('#BeginTime').val() == null) { BeginTime = ""; } else { BeginTime = $('#BeginTime').val(); }
        if ($('#EndTime').val() == null) { EndTime = ""; } else { EndTime = $('#EndTime').val(); }
        if ($('#searchStateCode').val() == null) { StateCode = ""; } else { StateCode = $('#searchStateCode').val(); }

        window.location.href = "/SmsDeviceState/CreateExcelToClient/"
                                + "?DeviceCode=" + $("#DeviceCode").val()
                                + "&BeginTime=" + $('#BeginTime').datebox('getValue')
                                + "&EndTime=" + $('#EndTime').datebox('getValue')
                                + "&BatchNo=" + $('#BatchNo').val()
                                + "&StateCode=" + $("#searchStateCode").val();
    }
</script>
