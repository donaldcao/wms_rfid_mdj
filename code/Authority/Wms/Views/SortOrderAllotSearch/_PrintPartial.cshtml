<script type="text/javascript">
    function printClick() {
        var orderDate = null,
        batchNo = null,
        sortingLineCode = null,
        deliverLineCode = null;

        if ($('#OrderDate').val() == null) { orderDate = ""; } else { orderDate = $('#OrderDate').val() }
        if ($('#BatchNo').val() == null) { batchNo = ""; } else { batchNo = $('#BatchNo').val() }
        if ($('#searchSortingLineCode').val() == null) { sortingLineCode = ""; } else { sortingLineCode = $('#searchSortingLineCode').val() }
        if ($('#deliverLineCode').val() == null) { deliverLineCode = ""; } else { deliverLineCode = $('#deliverLineCode').val() }

        window.location.href = "/SortOrderAllotSearch/CreateExcelToClient/"
                                + "?orderDate=" + $('#OrderDate').datebox('getValue')
                                + "&batchNo=" + $('#BatchNo').val()
                                + "&sortingLineCode=" + $("#searchSortingLineCode").val()
                                + "&deliverLineCode=" + $('#deliverLineCode').val();
    }
</script>
