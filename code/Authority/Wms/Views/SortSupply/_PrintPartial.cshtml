<script type="text/javascript">
    function printClick() {
        var sortBatchId = null,
            packNo = null,
            channelCode = null,
            productCode = null;

        if ($('#SortBatchId').datebox('getValue') == null) { sortBatchId = ""; } else { sortBatchId = $('#SortBatchId').datebox('getValue') }
        if ($('#PackNo').val() == null) { packNo = ""; } else { packNo = $('#PackNo').val() }
        if ($('#ChannelCode').val() == null) { channelCode = ""; } else { channelCode = $('#ChannelCode').val() }
        if ($('#searchProductCode').val() == null) { productCode = ""; } else { productCode = $('#searchProductCode').val() }

        window.location.href = "/SortSupply/CreateExcelToClient/"
                                + "?SortBatchId=" + sortBatchId
                                + "&PackNo=" + packNo
                                + "&ChannelCode=" + channelCode
                                + "&searchProductCode=" + productCode;
    }
</script>
