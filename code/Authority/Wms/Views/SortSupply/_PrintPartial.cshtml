<script type="text/javascript">
    function printClick() {
        var sortBatchId = null,
            packNo = null,
            channelCode = null,
            productCode = null;
        if ($('#SortBatchId').val() == null) { sortBatchId = ""; } else { sortBatchId = $('#SortBatchId').val() }
        if ($('#PackNo').val() == null) { packNo = ""; } else { packNo = $('#PackNo').val() }
        if ($('#searchChannelCode').val() == null) { channelCode = ""; } else { channelCode = $('#searchChannelCode').val() }
        if ($('#searchProductCode').val() == null) { productCode = ""; } else { productCode = $('#searchProductCode').val() }
        window.location.href = "/SortSupply/CreateExcelToClient/"
                                + "?SortBatchId=" + sortBatchId
                                + "&PackNo=" + packNo
                                + "&ChannelCode=" + channelCode
                                + "&ProductCode=" + productCode;
    }
</script>
