<script type="text/javascript">
    function printClick() {
        var sortSupplyCode = null, batchSortId = null, supplyId = null, packNo = null,
            channelCode = null; productCode = null; productName = null;

        if ($('#SortSupplyCode').val() == null) { sortSupplyCode = ""; } else { sortSupplyCode = $('#SortSupplyCode').val() }
        if ($('#BatchSortId').val() == null) { batchSortId = ""; } else { batchSortId = $('#BatchSortId').val() }
        if ($('#SupplyId').val() == null) { supplyId = ""; } else { supplyId = $('#SupplyId').val() }
        if ($('#PackNo').val() == null) { packNo = ""; } else { packNo = $('#PackNo').val() }
        if ($('#ChannelCode').val() == null) { channelCode = ""; } else { channelCode = $('#ChannelCode').val() }
        if ($('#ProductCode').val() == null) { productCode = ""; } else { productCode = $('#ProductCode').val() }
        if ($('#ProductName').val() == null) { productName = ""; } else { productName = $('#ProductName').val() }

        window.location.href = "/SortSupply/CreateExcelToClient/"
                                + "?sortSupplyCode=" + sortSupplyCode
                                + "&batchSortId=" + batchSortId
                                + "&supplyId=" + supplyId
                                + "&channelCode=" + channelCode
                                + "&packNo=" + packNo;
                                +"&productCode=" + productCode;
                                +"&productName=" + productName;
    }
</script>
