<script type="text/javascript">
    function printClick() {
        var OrderID = null,
        OrderDate = null,
        ProductCode = null;
        if ($('#OrderID').val() == null) { OrderID = ""; } else { OrderID = $('#OrderID').val(); }
        if ($('#OrderDate').val() == null) { OrderDate = ""; } else { OrderDate = $('#OrderDate').val(); }
        if ($('#productCode').val() == null) { ProductCode = ""; } else { ProductCode = $('#ProductCode').val(); }


        window.location.href = "/SortingOrder/CreateExcelToClient/"
                                + "?OrderID=" + $('#OrderID').val()
                                + "&OrderDate=" + $('#OrderDate').datebox('getValue')
                                + "&ProductCode=" + $('#productCode').val();
    }
</script>
