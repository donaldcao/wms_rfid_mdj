<script type="text/javascript">
    function printClick() {
        var billClass = null
          , isActive = null;
        if (document.getElementById('billClass1') == null) { billClass = ""; } else { billClass = $('#billClass1').combo('getValue'); }
        if (document.getElementById('IsActive') == null) { isActive = ""; } else { isActive = $('#IsActive').combo('getValue'); }
        window.location.href = "/StockBillType/CreateExcelToClient/"
                                + "?billClass=" + billClass
                                + "&isActive=" + isActive;
    }
</script>