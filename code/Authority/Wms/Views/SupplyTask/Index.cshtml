<div region="center" split="true" border="false">
    <table id="details" class="easyui-datagrid" url="/SupplyTask/Details/" border="false"
        singleselect="true" pagesize="50" fit="true" fitcolumns="true" toolbar="#tool"
        pagination="true" rownumbers="true">
        <thead>
            <tr>
                @*
                <th field="Id" width="30">
                    ID
                </th>
                *@
                <th field="SupplyId" width="60">
                    补货编码
                </th>
                <th field="PackNo" width="40">
                    烟包包号
                </th>
                <th field="SortingLineCode" width="80">
                    分拣线编码
                </th>
                <th field="SortingLineName" width="100">
                    分拣线名称
                </th>
                  <th field="GroupNo" width="60">
                    分拣线组号
                </th>
                <th field="ChannelCode" width="40">
                    烟道代码
                </th>
                <th field="ChannelName" width="80">
                    烟道名称
                </th>
                <th field="ProductCode" width="60">
                    商品代码
                </th>
                <th field="ProductName" width="130">
                    商品名称
                </th>
                <th field="ProductBarcode" width="130">
                    商品条码
                </th>
                  <th field="OriginPositionAddress" width="60">
                    起始位置
                </th>
                <th field="TargetSupplyAddress" width="60">
                    目标地址
                </th>
                <th field="Status" width="60">
                    状态
                </th>
            </tr>
        </thead>
    </table>
</div>
